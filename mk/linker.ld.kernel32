ENTRY(__zelda_boot_entry)
OUTPUT_FORMAT(elf32-i386)
OUTPUT_ARCH(i386:i386)

SECTIONS
{
  . = 0x0100000;


  .text BLOCK(4K) : ALIGN(4K)
  {
    *(.multiboot)
    *(.text*)
    *(.rodata)
  }
  .data BLOCK(4K) : ALIGN(4K)
  {
    *(.data)
  }
  . = 0x800000;
  .bss BLOCK(4K) : ALIGN(4K)
  {
    *(.raw_kernel64)
    *(.bss)
  }
  . = ALIGN(4096);
  /DISCARD/ : { *(.fini_array*) *(.comment) }
}

